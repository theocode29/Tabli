{
  "name": "tabli",
  "version": "1.0.0",
  "description": "Application PWA pour la gestion d'exercices de théâtre",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "postbuild": "node scripts/patch-build-index.js",
    "preview": "vite preview --outDir build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "dependencies": {
    "dexie": "^4.0.0",
    "svelte-dnd-action": "^0.11.8"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.43.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "svelte": "^5.39.5",
    "svelte-check": "^4.3.2",
    "typescript": "^5.9.2",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^0.20.0"
  },
  "keywords": [
    "svelte",
    "theatre",
    "exercises"
  ],
  "author": "Tabli Team",
  "license": "MIT"
}
